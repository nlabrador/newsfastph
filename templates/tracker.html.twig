  <script>
    $(document).ready( function () {
        $('#myTable').DataTable({
            "order": [[ 1, "desc" ]],
            "iDisplayLength": 100
        });

        $('.move-buttons').on('click', '#next', function() {
            $('#article-div').hide();
            index = $(this).attr('val');
            $(this).attr('val', parseInt(index) + 1);
            $('#prev').attr('val', parseInt($('#prev').attr('val')) + 1);

            $.get('/get/news/'+index, function( data ){
                news = data.news;
                $('#newsimg').attr('src', news.img);
                $('#newsdate').html('&nbsp;'+news.datetime);
                $('#newsname').text(news.name);
                $('#newsweb').text(news.website);
                $('#newshref').attr('href', news.href);
                $('#newshref').html(news.title);
                
                if (news.video) {
                    $('#newsimgvid').html('<br><iframe src="'+news.video+'" width="100%" height="250"></iframe>');
                }
                else if (news.artImg) {
                    $('#newsimgvid').html('<br><img class="img-fluid" alt="Image" src="'+news.artImg+'" style="max-width:100%;">');
                }
                else {
                    $('#newsimgvid').html('');
                }
                
                $('#newscontent').html(news.content+'<br><a target="_blank" href="'+ news.href +'">Read more</a>');
                $('#share').attr('href','https://facebook.com/sharer/sharer.php?u='+news.href);
                $('#share').show();

                $('#article-div').show();
                $('#prev').show();
            
                $("html, body").animate({ scrollTop: 0 }, "slow");
            });
        });

        $('.move-buttons').on('click', '#prev', function() {
            index = $(this).attr('val');
            if (parseInt(index) == 0) {
                $('#prev').hide();
                $(this).attr('val', 0);
                $('#next').attr('val', 1);
            }
            else {
                $(this).attr('val', parseInt(index) - 1);
                $('#next').attr('val', $('#next').attr('val') - 1);
            }

            $.get('/get/news/'+index, function( data ){
                news = data.news;
                $('#newsimg').attr('src', news.img);
                $('#newsdate').html('&nbsp;'+news.datetime);
                $('#newsname').text(news.name);
                $('#newsweb').text(news.website);
                $('#newshref').attr('href', news.href);
                $('#newshref').html(news.title);
                
                if (news.video) {
                    $('#newsimgvid').html('<br><iframe src="'+news.video+'" width="100%" height="250"></iframe>');
                }
                else if (news.artImg) {
                    $('#newsimgvid').html('<br><img class="img-fluid" alt="Image" src="'+news.artImg+'" style="max-width:100%;">');
                }
                else {
                    $('#newsimgvid').html('');
                }
                
                $('#newscontent').html(news.content+'<br><a target="_blank" href="'+ news.href +'">Read more</a>');

                $('#share').attr('href','https://facebook.com/sharer/sharer.php?u='+news.href);
                $('#share').show();

                $('#article-div').show();
                $("html, body").animate({ scrollTop: 0 }, "slow");
            });
        });
    } );
  </script>
  
